{
    "user": "Zeex",
    "repo": "samp-plugin-profiler",
    "resources": [
        {
            "name": "^profiler-(.*)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": ["."],
            "plugins": ["profiler.so"]
        },
        {
            "name": "^profiler-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["."],
            "plugins": ["profiler.dll"]
        }
    ],
    "contributors": ["Zeex"],
    "runtime": {
        "plugins": ["Zeex/profiler"]
    }
}